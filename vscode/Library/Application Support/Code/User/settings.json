{
  "editor.fontSize": 13,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.tabSize": 2,
  "editor.lineNumbers": "on",
  "editor.wordWrapColumn": 120,
  "editor.cursorBlinking": "solid",
  "editor.fontLigatures": true,
  "editor.linkedEditing": true,
  "editor.suggest.showStatusBar": true,
  "editor.renderWhitespace": "trailing",
  "editor.minimap.enabled": false,
  "files.refactoring.autoSave": false,
  "workbench.colorTheme": "Base16 Dark Railscasts",
  "workbench.enableExperiments": false,
  "workbench.experimental.editor.dropIntoEditor.enabled": false,
  "workbench.list.openMode": "doubleClick",
  "workbench.startupEditor": "none",
  "workbench.welcomePage.walkthroughs.openOnInstall": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.restoreViewState": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "window.nativeFullScreen": false,
  "window.restoreWindows": "none",
  "window.openFoldersInNewWindow": "on",
  "extensions.ignoreRecommendations": true,
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.defaultProfile.osx": "zsh (2)",
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.shellIntegration.showWelcome": false,
  "task.saveBeforeRun": "never",
  "problems.showCurrentInStatus": true,
  "audioCues.lineHasError": "off",
  "audioCues.lineHasBreakpoint": "off",
  "audioCues.lineHasInlineSuggestion": "off",
  "audioCues.noInlayHints": "off",
  "audioCues.onDebugBreak": "off",
  "keyboard.touchbar.enabled": false,
  "telemetry.telemetryLevel": "off",
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "html.autoClosingTags": true,
  "ruby.intellisense": "rubyLocate",
  "ruby.codeCompletion": "rcodetools",
  "ruby.useLanguageServer": true,
  "railsI18n.loadAllTranslations": true,
  "trailing-spaces.deleteModifiedLinesOnly": true,
  "trailing-spaces.highlightCurrentLine": true,
  "trailing-spaces.trimOnSave": true,
  "trailing-spaces.showStatusBarMessage": true,
  "remote.restoreForwardedPorts": false,
  "redhat.telemetry.enabled": false,
  "thunder-client.codeSnippetLanguage": "cs-httpclient"
}
